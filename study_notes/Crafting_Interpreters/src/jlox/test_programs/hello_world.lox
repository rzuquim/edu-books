fun fib(n) {
    // TODO: do some typechecking
    return noTypeCheckFib(n);
}

fun noTypeCheckFib(n) {
    if (n < 1) {
        return 0;
    }
    if (n <= 2) {
        return 1;
    }
    return noTypeCheckFib(n - 1) + noTypeCheckFib(n - 2);
}

